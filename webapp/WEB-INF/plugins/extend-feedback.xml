<?xml version="1.0" encoding="UTF-8"?><plug-in>

    <!-- Plugin Informations -->
    <name>extend-feedback</name>
    <class>fr.paris.lutece.plugins.extend.modules.feedback.service.FeedbackPlugin</class>
    <version>3.0.0</version>
    <description>module.extend.feedback.module.description</description>
    <provider>module.extend.feedback.module.provider</provider>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/extend/modules/feedback/extend-feedback.png</icon-url>
    <copyright>Copyright 2001-2025 Ville de Paris</copyright>
    <core-version-dependency>
        <min-core-version>8.0.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>

    <!-- if the plugin must have a connection pool with parameter : 1 - yes, 0 - no -->
    <db-pool-required>1</db-pool-required>
    
    <!-- Xpage configuration -->
    <applications>
        <application>
            <application-id>extend-feedback</application-id>
        </application>
    </applications>
    <admin-features>
        <admin-feature>
            <feature-id>FEEDBACK_TYPE_MANAGEMENT</feature-id>
            <feature-title>module.extend.feedback.module.manageFeedbackType.name</feature-title>
            <feature-description>module.extend.feedback.module.manageFeedbackType.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/extend/modules/feedback/ManageFeedbackType.jsp</feature-url>
            <feature-icon-url/>
        </admin-feature>
        <admin-feature>
            <feature-id>FEEDBACK_MANAGEMENT</feature-id>
            <feature-title>module.extend.feedback.module.manageFeedback.name</feature-title>
            <feature-description>module.extend.feedback.module.manageFeedback.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/extend/modules/feedback/ManageFeedback.jsp</feature-url>
            <feature-icon-url/>
        </admin-feature>
    </admin-features>
</plug-in>